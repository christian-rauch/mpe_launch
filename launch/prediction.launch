<launch>
  <arg name="MODEL"/>
  <arg name="IMG_COLOUR"/>
  <arg name="IMG_DEPTH"/>

  <node name="mpe" pkg="mpe_prediction" type="mpe_predict.py" args="$(arg MODEL)" output="screen">
    <remap from="colour/compressed" to="$(arg IMG_COLOUR)"/>
    <remap from="depth/compressedDepth" to="$(arg IMG_DEPTH)"/>
  </node>
</launch>
